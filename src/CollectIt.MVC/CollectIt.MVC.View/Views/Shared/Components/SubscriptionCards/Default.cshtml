@model CollectIt.MVC.View.Views.Shared.Components.SubscriptionCards.SubscriptionCardsViewModel

<ul class="text-decoration-none my-1 d-flex justify-content-center flex-xl-row flex-column">
    @foreach (var card in Model.Subscriptions)
    {
        <li>
            <vc:subscription-card subscription="@card"/>
        </li>
    }
</ul>

